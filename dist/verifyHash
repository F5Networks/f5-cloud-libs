cli script /Common/verifyHash {
proc script::run {} {
        if {[catch {
            set hashes(f5-cloud-libs.tar.gz) 4f02c71f17e85c8f8d59bd9b262d828646ce5d7fa4cdc4d887d8d1f5342b86a1de002425983e0ab9c572b8f31948350d155db3314ee884c83b89551e22df9f79
            set hashes(f5-cloud-libs-aws.tar.gz) 2566f515fb46d89f5a245079b0efdad60fd78327c352e567bd5d573eb2ee0093d167a2f054b2408bd7df49c5debc4218074fdb50cfe135bb80ccc6c303a03f72
            set hashes(f5-cloud-libs-azure.tar.gz) 90e9162245f204b0c15e5b30c8d7e9d5f110a03002c0bfeaa83e51252119bfc1c4f711dc85d630c8203dab4a6ec373ae2c3777586a3fb1631dc61d7fa095448e
            set hashes(asm-policy-linux.tar.gz) 63b5c2a51ca09c43bd89af3773bbab87c71a6e7f6ad9410b229b4e0a1c483d46f1a9fff39d9944041b02ee9260724027414de592e99f4c2475415323e18a72e0
